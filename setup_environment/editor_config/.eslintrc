{
  "plugins": ["prettier", "react"],
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "prettier/prettier": "error",
    "no-undef" : 2,
    "comma-dangle": 0,
    "react/jsx-uses-vars": 1,
    "react/display-name": 1,
    "no-unused-vars": "warn",
    "no-console": 1,
    "no-unexpected-multiline": "warn",
    "react/prop-types": 0
  },
  "settings": {
    "react": {
      "version": "detetect"
    }
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "spread" : true,
      "restParams" : true,
      "experimentalObjectRestSpread": true
    },
  },
  "env": {
    "commonjs": true,
    "es6": true,
    "browser" : true,
    "node" : true,
  },
  "globals": {
      "_": false
  }
}
